    <string name="help_dialog_add_quest_text1">Create your quests using natural everyday language.</string>
    <string name="help_dialog_add_quest_text2">You can create one time quest or repeating one.</string>
    <string name="help_dialog_add_quest_text3">Start typing and use the smart suggestions from the dropdown to help you.</string>
